{
  "expo": {
    "name": "Scarletts Gesundheitstracking",
    "slug": "scarlett-gesundheitstracking",
    "version": "1.2.23",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "scarlett-health",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": { "supportsTablet": true },
    "android": {
      "package": "de.scarlett.gesundheitstracking",
      "versionCode": 18,
      "adaptiveIcon": { "foregroundImage": "./assets/images/adaptive-icon.png", "backgroundColor": "#ffffff" },
      "enableProguardInReleaseBuilds": true,
      "edgeToEdgeEnabled": true,
      "allowBackup": false
    },
    "web": { "bundler": "metro", "output": "static", "favicon": "./assets/images/favicon.png" },
    "plugins": [
      "expo-router",
      "expo-notifications",
      ["expo-splash-screen", { "image": "./assets/images/icon.png", "imageWidth": 220, "resizeMode": "contain", "backgroundColor": "#fde7ef", "dark": { "image": "./assets/images/icon.png", "backgroundColor": "#1b0b12" } }]
    ],
    "assetBundlePatterns": ["assets/images/*"],
    "experiments": { "typedRoutes": true },
    "extra": {
      "EXPO_PUBLIC_EMERGENT_LLM_KEY": "sk-emergent-e34Af18EdBf12063f7"
    }
  }
}